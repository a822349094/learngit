   "source": [
    "import cv2\n",
    "\n",
    "cap=cv2.VideoCapture(\"38.mp4\")\n",
    "print(cap.get(cv2.CAP_PROP_FRAME_WIDTH))\n",
    "while(cap.isOpened()):\n",
    "    ret,frame=cap.read()\n",
    "    cv2.imshow('video',frame)\n",
    "    c=cv2.waitKey(1)\n",
    "    if c==27:\n",
    "        break\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()\n"
   ]
